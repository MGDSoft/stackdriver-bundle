{
    "name": "mgdsoft/stackdriver-bundle",
    "type": "symfony-bundle",
    "description": "Google Stackdriver monolog handler & error reporting",
    "keywords": ["gcloud", "error reporting"],
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel García Domínguez",
            "email": "miguelgd@mgdsoftware.es"
        }
    ],
    "autoload":{
        "psr-4":{
            "MGDSoft\\Stackdriver\\": ""
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "~7.0",
        "ext-json": "*",
        "symfony/security-bundle": "^4.0 || ^5.0",
        "symfony/monolog-bundle": "^3.6",
        "google/cloud": ">=0.111.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/yaml": "^5.2"
    }
}
